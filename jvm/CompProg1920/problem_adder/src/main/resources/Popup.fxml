<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="120.0" prefWidth="500.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="npj.PopupController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <children>
        <HBox prefHeight="70.0" prefWidth="200.0" spacing="10.0">
            <children>
                <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="50.0" spacing="10.0">
                    <children>
                        <Label prefHeight="25.0" text="Name:"/>
                        <Label prefHeight="25.0" text="URL:"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="10.0" HBox.hgrow="ALWAYS">
                    <children>
                        <TextField fx:id="nameTextField"/>
                        <TextField fx:id="urlTextField"/>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onCancel" text="Cancel"/>
                <Button mnemonicParsing="false" onAction="#onOK" prefWidth="52.0" text="OK"/>
            </children>
        </HBox>
    </children>
</VBox>
